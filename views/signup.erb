<div class = "signup">
 <form action="/signup" method = "post">
 		<legend>User Name</legend>
	 		<input type="text" placeholder="UserName" name="username" required>
	  <legend>Email</legend>
		  <input type="text" placholder="@" name="email" required>
	  <legend>Password</legend>
		  <input type="password" placeholder="Password" name="password" required>
	  <legend>Select Your Neighborhood</legend>
		  <select name="neighborhood" placeholder="DC Neighborhoods">
		     <option value = "capitolhill">Capitol Hill</option>
		     <option value = "clevelandwoodley">Cleveland/Woodley Park</option>
		     <option value = "glovergeorgetown">Gloverpark/Georgetown</option>
		     <option value = "mtpleasantcolumbiaheights">Mt. Pleasant/Columbia Heights</option>
		     <option value = "dupontcircle">Dupont Circle</option>
		     <option value = "shawbloomingdale">Shaw/Bloomingdale</option>
		     <option value = "tenleytown">Tenleytown/Friendship Heights</option>
		     <option value = "petworth">Petworth</option>
		     <option value = "takoma">Takoma Park</option>
		  </select>
		<legend>Submit</legend>
		  <input class = "fs" type="submit" placeholder="Log In">
 </form>
<a href="/">Home Page</a>
</div>
